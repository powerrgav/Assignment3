
public class Commercial extends Customer {
	private String homePhone;
	private String workPhone;
	
	public Commercial(String name, String address, String phone,String customerBranch, String homePhone, String workPhone) {
		super(name, address, phone, customerBranch);
		this.homePhone = homePhone;
		this.workPhone = workPhone;
		// TODO Auto-generated constructor stub
	}

	/**
	 * @return the homePhone
	 */
	public String getHomePhone() {
		return homePhone;
	}

	/**
	 * @param homePhone the homePhone to set
	 */
	public void setHomePhone(String homePhone) {
		this.homePhone = homePhone;
	}

	/**
	 * @return the workPhone
	 */
	public String getWorkPhone() {
		return workPhone;
	}

	/**
	 * @param workPhone the workPhone to set
	 */
	public void setWorkPhone(String workPhone) {
		this.workPhone = workPhone;
	}

	@Override
	public String toString() {
		return "Commercial\nHome Phone: " + homePhone + "\nWork Phone: " + workPhone + "\nName: " + name + "\nAddress: "
				+ address + "\nPhone: " + phone + "\nCustomer Branch: " + customerBranch;
	}




}

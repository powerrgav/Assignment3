
public class Cars {
	
	//declare variables
	protected String make;
	protected String model;
	protected int year;
	protected int vin;
	protected CarOwners owner;
	/**
	 * @param make
	 * @param model
	 * @param year
	 * @param vin
	 */
	public Cars(String make, String model, int year, int vin, CarOwners owner) {
		super();
		this.make = make;
		this.model = model;
		this.year = year;
		this.vin = vin;
		this.owner = owner;
	}
	/**
	 * @return the make
	 */
	public String getMake() {
		return make;
	}
	/**
	 * @param make the make to set
	 */
	public void setMake(String make) {
		this.make = make;
	}
	/**
	 * @return the model
	 */
	public String getModel() {
		return model;
	}
	/**
	 * @param model the model to set
	 */
	public void setModel(String model) {
		this.model = model;
	}
	/**
	 * @return the year
	 */
	public int getYear() {
		return year;
	}
	/**
	 * @param year the year to set
	 */
	public void setYear(int year) {
		this.year = year;
	}
	/**
	 * @return the vin
	 */
	public int getVin() {
		return vin;
	}
	/**
	 * @param vin the vin to set
	 */
	public void setVin(int vin) {
		this.vin = vin;
	}
	/**
	 * @return the owner
	 */
	public CarOwners getOwner() {
		return owner;
	}
	/**
	 * @param owner the owner to set
	 */
	public void setOwner(CarOwners owner) {
		this.owner = owner;
	}
	
	
}
